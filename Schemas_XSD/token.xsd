<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:element name="Token">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Token" type="Token"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="Token">
		<xs:sequence>
			<xs:element name="AccessToken" type="xs:string" minOccurs="0"/>
			<xs:element name="TokenType" type="xs:string" minOccurs="0"/>
			<xs:element name="ExpiresIn" type="xs:string" minOccurs="0"/>
			<xs:element name="RefreshToken" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
