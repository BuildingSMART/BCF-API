<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:element name="Auth">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Auth" type="Auth"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="Auth">
		<xs:sequence>
			<xs:element name="Oauth2AuthUrl" type="xs:string" minOccurs="0"/>
