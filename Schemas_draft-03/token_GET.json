{
    "$schema": "http://json-schema.org/draft-03/schema#",
    "title": "token",
    "description": "Access Token in BCF REST API.",
    "type": "object",
    "properties": {
        "access_token": {
            "type": "string",
            "required": true
        },
        "token_type": {
            "type": "string",
            "required": true
        },
        "expires_in": {
            "type": "number",
            "required": true
        },
        "refresh_token": {
            "type": "string",
            "required": true
        }
    }
}
